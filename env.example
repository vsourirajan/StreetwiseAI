# Copy this file to .env and fill in your API keys and dataset IDs

# Required API Keys
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=citybrain-zoning

# Optional: Census API Key (for demographics data)
CENSUS_API_KEY=your_census_api_key_here

# NYC Open Data Socrata Dataset IDs
# Find these on https://data.cityofnewyork.us/
NYC_ZONING_SOCRATA_DATASET_ID=mm69-vrje
NYC_TRAFFIC_SOCRATA_DATASET_ID=ertz-hr4r

# OSM place to download network for (use small area during development)
OSM_PLACE_NAME=Manhattan, New York, USA

# Base data directory (relative path is fine)
DATA_DIR=data

# Optional: Pinecone environment (usually us-east-1-aws)
PINECONE_ENVIRONMENT=us-east-1-aws

# Optional: Hugging Face embedding model (default: all-MiniLM-L6-v2)
# HF_EMBEDDING_MODEL=all-MiniLM-L6-v2 